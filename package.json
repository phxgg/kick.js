{
  "name": "kick.js",
  "version": "1.0.0",
  "description": "A JavaScript library for interacting with the Kick API.",
  "license": "SEE LICENSE IN LICENSE",
  "author": "phxgg",
  "main": "dist/app.js",
  "engines": {
    "node": ">=22.15.0"
  },
  "scripts": {
    "ncu:check": "npx npm-check-updates -x @types/node",
    "ncu:update": "npx npm-check-updates -u -x @types/node",
    "bump-deps": "npm run ncu:update && npm i && git add . && git commit -m \"chore: bump deps\"",
    "clean": "rimraf tsconfig.*.tsbuildinfo ./dist",
    "dev": "tsx watch ./src/app.ts",
    "start": "node ./dist/app.js",
    "type:check": "tsc --noEmit",
    "build:tsc": "npm run clean && tsc && tsc-alias",
    "build": "npm run clean && npm run type:check && swc src -d dist --copy-files --strip-leading-paths",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "dotenv": "^17.2.2",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.1",
    "morgan": "^1.10.1",
    "ms": "^2.1.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.8.0",
    "ulid": "^3.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-oauth2": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  }
}
