{
  "name": "kick.js",
  "version": "1.0.0",
  "description": "A JavaScript library for interacting with the Kick API.",
  "license": "MIT",
  "author": "phxgg",
  "main": "dist/app.js",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "ncu:check": "npx npm-check-updates -x @types/node",
    "ncu:update": "npx npm-check-updates -u -x @types/node",
    "bump-deps": "npm run ncu:update && npm i && git add . && git commit -m \"chore: bump deps\"",
    "clean": "rimraf tsconfig.tsbuildinfo ./dist",
    "dev": "tsx watch ./src/app.ts",
    "start": "node ./dist/app.js",
    "type:check": "tsc --noEmit",
    "build:tsc": "npm run clean && tsc && tsc-alias",
    "build": "npm run clean && npm run type:check && swc src -d dist --copy-files --strip-leading-paths",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.4.0",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.0.1",
    "morgan": "^1.10.1",
    "ms": "^2.1.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.8.0",
    "ulid": "^3.0.2",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.5",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-oauth2": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  }
}
